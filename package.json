{
  "name": "react-flow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "flow-bin": "^0.96.0",
    "flow-typed": "^2.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "flow": "flow",
    "flow:deps": "flow-typed install",
    "flow:file": "flow coverage src/App.js --color",
    "flow:cover": "flow-coverage-report --config ./flowcoverage.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "flow-coverage-report": "^0.6.1"
  }
}
