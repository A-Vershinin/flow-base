{
  "name": "react-flow",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "css-module-flow": "^1.0.0",
    "flow-bin": "^0.96.0",
    "flow-typed": "^2.5.1",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "flow": "flow",
    "flow:deps": "flow-typed install --skip",
    "flow:file": "flow coverage src/utils/index.js --color",
    "flow:cover": "flow-coverage-report --exclude-non-flow --config ./flowcoverage.json",
    "flow:test": "flow-coverage-report --exclude-non-flow --config ./flowcoverage.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "flow-coverage-report": "^0.6.1"
  },
  "moduleRoots": [
    "src/"
  ]
}
